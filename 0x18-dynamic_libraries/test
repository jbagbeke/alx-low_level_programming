#include "main.h"

int main()
{
	int c, n;
	char s, *str1, *str2;
	unsigned int b;
_putchar(c);
_islower(c);
_isalpha(c);
_abs(n);
_isupper(c);
_isdigit(c);
_strlen(str1);
_puts(str1);
_strcpy(str1, str2);
_atoi(str1);
_strcat(str1, str2);
_strncat(str1, str2, n);
_strncpy(str2, str1, n);
_strcmp(str1, str2);
_memset(str1, s, b);
_memcpy(str1, str2, n);
_strchr(str1, s);
_strspn(str1, str2);
_strpbrk(str1, str2);
_strstr(str1, str2);

return (0);
}
